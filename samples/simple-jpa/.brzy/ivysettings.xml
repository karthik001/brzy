<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
  <settings defaultResolver="chained"/>
  <resolvers>
    <chain name="chained">
      <filesystem name="1">
        <artifact pattern="${basedir}/.brzy/lib2/[artifact]-[revision].[ext]"/>
      </filesystem>
      <ibiblio name="ibiblio" />
      <ibiblio name="maven" m2compatible="true" usepoms="false"/>
      <ibiblio name="java-net2" root="http://download.java.net/maven/2" m2compatible="true"/>
			<ibiblio name="java-net1" root="http://download.java.net/maven/1" />
      <ibiblio name="scala-tools" root="http://scala-tools.org/repo-releases" m2compatible="true"/>
      <ibiblio name="jboss" root="http://repository.jboss.com/maven2" m2compatible="true"/>
      <ibiblio name="fusesource.snapshots" root="http://repo.fusesource.com/nexus/content/repositories/snapshots" m2compatible="true"/>
    </chain>
  </resolvers>
  <!--<modules>-->
    <!--<module organisation="jayasoft" name=".*" resolver="internal" />-->
  <!--</modules>-->
</ivysettings>